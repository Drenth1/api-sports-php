{
    "name": "drenth1/api-sports-php",
    "description": "A robust wrapper around all APIs provided by api-sports.io",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drenth1\\ApiSports\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drenth1\\ApiSports\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Joey Drenth",
            "email": "drenthjoey@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests --testdox"
    }
}
